      "source": [
        "# Next we are going to do the same steps as above, but for longitude\n",
        "# Filter out longitude entries of 0\n",
        "df_filtered_longitude = df[df['LONGITUDE'] != 0.0]\n",
        "\n",
        "# Find the lowest and highest longitude numbers after filtering\n",
        "highest_longitude = df_filtered_longitude['LONGITUDE'].max()\n",
        "lowest_longitude = df_filtered_longitude['LONGITUDE'].min()\n",
        "\n",
        "# Round the lowest and highest longitude numbers to second decimal place & print the values\n",
        "highest_longitude_round = round(highest_longitude, 2)\n",
        "lowest_longitude_round = round(lowest_longitude, 2)\n",
        "print(\"Highest Longitude number:\", highest_longitude_round)\n",
        "print(\"Lowest Longitude number:\", lowest_longitude_round)\n",
        "print(\"\")\n",
        "\n",
        "# Create a list of longitude numbers between the lowest and highest, round to the second decimal place\n",
        "longitude_numbers = sorted(df_filtered_longitude['LONGITUDE'].round(2).unique())\n",
        "df_filtered_longitude['Rounded Longitude'] = df_filtered_longitude['LONGITUDE'].round(2)\n",
        "entries_per_longitude = df_filtered_longitude['Rounded Longitude'].value_counts()\n",
        "sorted_longitudes = sorted(entries_per_longitude.items(), key=lambda x: x[0])\n",
        "\n",
        "# Print the count of entries for each rounded longitude number, sorted by longitude\n",
        "print(\"Entries per longitude number (rounded to second decimal place):\")\n",
        "for longitude, count in sorted_longitudes:\n",
        "    print(f\"Entries For Longitude {longitude}: {count}\")"
      ],

        "# With this, we will sort and display the amount of environmental complaints each year\n",
        "# Convert the date format & extract year\n",
        "df['COMPLAINT DATE'] = pd.to_datetime(df['COMPLAINT DATE'], format='%m/%d/%Y')\n",
        "df['Year'] = df['COMPLAINT DATE'].dt.year\n",
        "\n",
        "# Count entries for each year & sort years in order\n",
        "entries_per_year = df['Year'].value_counts()\n",
        "entries_per_year_sorted = entries_per_year.sort_index()\n",
        "print(\"Entries Each Year:\")\n",
        "print(entries_per_year_sorted)"

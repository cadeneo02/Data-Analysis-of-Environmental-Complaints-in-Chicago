        "# Next, I am going to conbine the two DataFrames\n",
        "# Use concat() for the two DataFrames vertically\n",
        "combined_df = pd.concat([df_master, df_2_master], ignore_index=True)\n",
        "\n",
        "# Print the shape of the combined DataFrame to verify the number of rows\n",
        "print(\"Shape of DataFrame:\", combined_df.shape)\n",
        "\n",

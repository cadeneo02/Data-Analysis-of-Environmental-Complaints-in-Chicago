      "cell_type": "code",
      "source": [
        "# Next, we will calculate and display how much of each complaint there are in the dataframe\n",
        "# Calculate percentage of each type\n",
        "complaint_type_count = df['COMPLAINT TYPE'].value_counts(normalize=True) * 100\n",
        "\n",
        "# Combine complaint types with less than 2% into 'Other'\n",
        "threshold = 2\n",
        "other_complaints = complaint_type_count[complaint_type_count < threshold]\n",
        "other_complaints_total = other_complaints.sum()\n",
        "complaint_type_count = complaint_type_count[complaint_type_count >= threshold]\n",
        "complaint_type_count['Other'] = other_complaints_total\n",
        "\n",
        "# Plot data, create & display chart\n",
        "plt.figure(figsize=(10, 6))\n",
        "plt.pie(complaint_type_count, labels=complaint_type_count.index, autopct='%1.1f%%', startangle=140)\n",
        "plt.title('Environmental Complaint Types')\n",
        "plt.axis('equal')\n",
        "plt.show()"
      ],

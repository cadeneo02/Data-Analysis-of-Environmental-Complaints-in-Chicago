        "# Now we will create a chart just like the last to show the amount of environmental complaints per longitude.\n",
        "# Because latitude and longitude are so specific, we will group them to the second decimal place.\n",
        "# Filter longitude values within the range of -88 to -87, filter to second decimal place\n",
        "filtered_df = df[(df['LONGITUDE'] >= -88) & (df['LONGITUDE'] <= -87)]\n",
        "longitude_groups = filtered_df['LONGITUDE'].round(2)\n",
        "\n",
        "# Count complaints in each longitude group & sort them\n",
        "complaints_per_longitude_group = longitude_groups.value_counts()\n",
        "sorted_longitude_groups = sorted(complaints_per_longitude_group.index)\n",
        "\n",
        "# Plot data with titles and lables, as well as setting axis ranges\n",
        "plt.figure(figsize=(10, 6))\n",
        "plt.bar(sorted_longitude_groups, complaints_per_longitude_group[sorted_longitude_groups], color='blue')\n",
        "plt.title('Number of Complaints Per Longitude Group')\n",
        "plt.xlabel('Longitude Group')\n",
        "plt.ylabel('Number of Complaints')\n",
        "plt.ylim(0, 6000)\n",
        "\n",
        "# Display plot\n",
        "plt.xticks(rotation=90)\n",
        "plt.show()"
